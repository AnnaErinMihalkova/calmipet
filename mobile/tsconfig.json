{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "moduleResolution": "node16",
    "target": "ES2019",
    "module": "Node16",
    "lib": ["ESNext", "DOM"],
    "jsx": "react-jsx",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "strict": true,
    "resolveJsonModule": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src", "App.tsx", "index.ts"]
}
